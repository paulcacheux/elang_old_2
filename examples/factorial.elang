//Calculate the factorial of a number
//120

BEGIN
  factorialOf = 5

  IF factorialOf > 0
    x = factorialOf /*
      This comment
      spans multiple lines! ()__()
    */
    y = 1

    LOOP
      IF x > 0
        y = y * x
        x = x - 1
      ELSE
        BREAK
      END
    END

    print(y)
  ELSE
    print(-1 /*Typical comment*/)
  END
END

