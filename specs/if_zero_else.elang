//Branch to else on non-zero values with ifZ
//116
//827
//1130
//115

fn main()
{
  if 0 == 0
    println(116);
  else
    println(1);

  if 1 == 0
    println(2);
  else
    println(827);

  let x = 0;
  let y = 0;

  if x == 0
    println(1130);
  else
    println(3);

  if y+1 == 0
    println(4);
  else
    println(115);
}
