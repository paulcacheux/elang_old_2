//Branch to else on non-negative values with ifN
//116
//827
//1130
//115

fn main()
{
  if 0 < 0
    println(1);
  else
    println(116);

  if -1 < 0
    println(827);
  else
    println(2);

  let x = 0;
  let y = 0;

  if x < 0
    println(3);
  else
    println(1130);

  if y - 1 < 0
    println(115);
  else
    println(4);
}
