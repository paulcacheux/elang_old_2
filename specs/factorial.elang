//Calculate the factorial of a number
//120

fn main()
{
  let factorialOf = 5;

  if factorialOf > 0 {
    let x = factorialOf;
    let y = 1;

    loop {
      if x > 0 {
        y = y * x;
        x = x - 1;
      } else {
        break;
      }
    }

    println(y);
  } else {
    println(-1);
  }
}
